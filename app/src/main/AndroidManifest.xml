<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="lab5"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MobileSystems"
        tools:targetApi="31">

        <receiver
            android:name=".ActivityNotificationClicker$NotificationReceiver"
            android:exported="true"
            android:permission="TODO">
            <intent-filter>
                <action android:name="increase" />
                <action android:name="decrease" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".ActivityNotificationClicker"
            android:exported="false"
            android:label="Activity Notification Clicker"/>

        <activity
            android:name=".ActivityUpDwMenu"
            android:exported="false"
            android:label="Activity Up Dw Menu"/>
        <activity
            android:name=".ActivityWebView"
            android:exported="false"
            android:label="Activity Web View"/>
        <activity
            android:name=".ActivityListMenu"
            android:exported="false"
            android:label="Activity List Menu"/>
        <activity
            android:name=".ActivityWeeklyTasks"
            android:exported="false"
            android:label="Activity Weekly Tasks" />
        <activity
            android:name=".ActivityTimePicker"
            android:exported="false"
            android:label="Activity Time Picker" />
        <activity
            android:name=".ActivityTextPicker"
            android:exported="false"
            android:label="Activity Text Picker" />
        <activity
            android:name=".ActivityDataPicker"
            android:exported="false"
            android:label="Activity Data Picker" />

        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="lab5">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ActivityPageStack"
            android:exported="false"
            android:label="Activity Page Stack" />
    </application>

</manifest>